{
    "version": 2,
    "builds": [
      {
        "src": "src/app.ts",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 10,
          "includeFiles": ["src/**/*", "prisma/**/*", "package.json", "pnpm-lock.yaml"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.ts"
      }
    ],
    "functions": {
      "src/app.ts": {
        "maxDuration": 10
      }
    }
  }
  